{
  "tasks": {
    "dev": "deno run -A vite",
    "server": "deno run --allow-read --allow-net ./src/serve.ts",
    "build": "deno run -A vite build && deno run server"
  },
  "imports": {
    "@std/http": "jsr:@std/http@^1.0.19",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@^6.0.0",
    "bootstrap": "npm:bootstrap@^5.3.7",
    "bootstrap-icons": "npm:bootstrap-icons@^1.13.1",
    "sass": "npm:sass@^1.89.2",
    "upng-js": "npm:upng-js@^2.1.0",
    "vite": "npm:vite@^7.0.4",
    "vue": "npm:vue@^3.5.17"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "preserve",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  }
}
